{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["translate"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "store:default",
    "store:allow-create-store",
    "core:window:allow-start-dragging",
    "core:window:allow-is-focused",
    "core:window:deny-is-focused"
  ]
}
